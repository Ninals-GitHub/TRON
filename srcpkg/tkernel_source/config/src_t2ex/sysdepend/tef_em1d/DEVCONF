#
#	DEVCONF (T-Engine/EM1D-512)
#
#	Device configuration
#

#
# Use DCTAG_XXX as a parameter of GetDevConf().
# DCTAG_XXX is defined in <sys/rominfo.h>.
#

# Debug mode (1:debug mode, 0:normal mode)
DEBUGMODE	1

# Network Driver (device type, I/O address, IRQ)
#	device type  = 0x00011	SMSC LAN9118
#	device type |= 0x00800	always power-on
#	device type |= 0x10000	full-duplex mode
NETDEV0	0x00011 0x28000000 172
